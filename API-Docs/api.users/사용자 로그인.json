{
  "method": "post",
  "uri": "/api/user/signin",
  "request": {
    "body": {
      "email": "String",
      "password": "String"
    }
  },
  "response": {
    "success": {
      "status": "200 Ok",
      "headers": {
        "cookies": { "token": "String" }
      },
      "data": { "ok": true }
    },
    "errors": [
      {
        "status": "400 Bad Request",
        "data": {
          "ok": false,
          "message": "이메일 형식을 확인하세요."
        }
      },
      {
        "status": "400 Bad Request",
        "data": {
          "ok": false,
          "message": "비밀번호 형식을 확인하세요."
        }
      },
      {
        "status": "401 Unauthorized",
        "data": {
          "ok": false,
          "message": "이메일 또는 비밀번호를 확인하세요."
        }
      },
      {
        "status": "500 Internal Server Error"
      }
    ]
  }
}
